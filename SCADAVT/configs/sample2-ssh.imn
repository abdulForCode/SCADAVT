annotation a0 {
    iconcoords {202 75 612 405}
    type rectangle
    label {provider network}
    labelcolor black
    fontfamily {Arial}
    fontsize 10
    color #f8f8d6
    width 0
    border black
    rad 25
    canvas c0
}

canvas c0 {
    name {Canvas0}
}

option global {
    interface_names no
    ip_addresses yes
    ipv6_addresses yes
    node_labels yes
    link_labels yes
    ipsec_configs yes
    remote_exec no
    exec_errors yes
    show_api no
    background_images no
    annotations yes
    grid yes
}

